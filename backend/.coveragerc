[run]
source = accounts
omit =
    */migrations/*
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */env/*
    */virtualenv/*
    */site-packages/*
    manage.py
    */wsgi.py
    */asgi.py
    */settings.py
    */apps.py

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov
title = Accounts App Coverage Report

[xml]
output = coverage.xml
